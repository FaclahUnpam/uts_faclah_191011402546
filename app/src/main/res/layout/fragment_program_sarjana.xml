<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".ProgramSarjanaFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="0dp"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="19dp">

        <ImageView
            android:id="@+id/Komunikasi"
            android:layout_width="122dp"
            android:layout_height="117dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:src="@drawable/fikom1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="16dp" />

        <ImageView
            android:id="@+id/Teknologiinformasi"
            android:layout_width="122dp"
            android:layout_height="117dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="12dp"
            android:src="@drawable/komputer1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Komunikasi"
            android:layout_marginLeft="16dp" />

        <ImageView
            android:id="@+id/Teknik"
            android:layout_width="122dp"
            android:layout_height="117dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="21dp"
            android:src="@drawable/administrator"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Teknologiinformasi"
            android:layout_marginLeft="16dp" />

        <ImageView
            android:id="@+id/Ekobisnis"
            android:layout_width="122dp"
            android:layout_height="117dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="40dp"
            android:src="@drawable/fisip1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/SosialPolitik"
            android:layout_marginLeft="16dp" />

        <ImageView
            android:id="@+id/SosialPolitik"
            android:layout_width="122dp"
            android:layout_height="117dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="22dp"
            android:src="@drawable/ekonomibisnis"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Teknik"
            android:layout_marginLeft="16dp" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="236dp"
            android:layout_height="40dp"
            android:layout_marginTop="63dp"
            android:layout_marginEnd="25dp"
            android:text="Fakultas Ilmu Komunikasi"
            android:textColor="#050505"
            android:textSize="20sp"
            android:textStyle="bold|italic"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginRight="25dp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="245dp"
            android:layout_height="61dp"
            android:layout_marginTop="78dp"
            android:layout_marginEnd="16dp"
            android:text="Fakultas Teknologi Informasi"
            android:textColor="#050505"
            android:textSize="20sp"
            android:textStyle="bold|italic"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            android:layout_marginRight="16dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="245dp"
            android:layout_height="61dp"
            android:layout_marginTop="69dp"
            android:layout_marginEnd="16dp"
            android:text="Fakultas Teknik"
            android:textColor="#050505"
            android:textSize="20sp"
            android:textStyle="bold|italic"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView3"
            android:layout_marginRight="16dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="245dp"
            android:layout_height="61dp"
            android:layout_marginTop="85dp"
            android:layout_marginEnd="16dp"
            android:text="Fakultas Ekonomi dan Bisnis"
            android:textColor="#050505"
            android:textSize="20sp"
            android:textStyle="bold|italic"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView4"
            android:layout_marginRight="16dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="245dp"
            android:layout_height="61dp"
            android:layout_marginTop="93dp"
            android:layout_marginEnd="16dp"
            android:text="Fakultas Ilmu Sosial dan Bisnis"
            android:textColor="#050505"
            android:textSize="20sp"
            android:textStyle="bold|italic"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView5"
            android:layout_marginRight="16dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>


</ScrollView>

